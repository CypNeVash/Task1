@{
    ViewBag.Title = "Home Page";
}
@using Blogs.Model.Article;

@Styles.Render("~/Content/css")

@model IEnumerable<Article>

@foreach (var item in Model)
{
    <div class="article">
        <h2>@item.Title</h2>
        <p>
            @item.Preview
        </p>
        <p class="links"> @Html.ActionLink("Read more", "Index", "Guests", new { id = item.Id }, null)</p>
    </div>
}